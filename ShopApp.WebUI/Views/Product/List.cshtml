@model ProductViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List</title>
</head>
<body>
 
   @foreach (var item in Model.products)
   {
     <h1>@Model.categorys.Name</h1>
      <ul>
        <li>@item.Name</li>
        <li>@item.Price</li>
        <li>@item.Description</li>
      </ul>
   }
</body>
</html>