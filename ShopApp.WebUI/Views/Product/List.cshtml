@model List<Product>
@{
  
  Layout ="_layout";
}
<div class="row">
  <div class="col-md-3">
    @await Component.InvokeAsync("Category")
</div>
<div class="col-md-9 mb-5">
    <div class="row">
      @if (Model.Count != 0)
      {
          @foreach (var item in Model)
          {
            <div class="col-md-4 p-1">
              @await Html.PartialAsync("_product",item)
            </div>
          }
      }
      else{
        @await Html.PartialAsync("_noproduct")
      }
  </div>
</div>

 